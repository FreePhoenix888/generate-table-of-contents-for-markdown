{
  "name": "@freephoenix888/generate-table-of-contents-for-markdown",
  "version": "4.0.8",
  "description": "Generates a table of contents for a markdown file",
  "type": "module",
  "main": "dist/main.js",
  "types": "dist/main.d.ts",
  "repository": {
    "url": "https://github.com/FreePhoenix888/generate-table-of-contents-for-markdown"
  },
  "scripts": {
    "library:build:clean": "fse remove dist && npm run library:build:dirty",
    "library:build:dirty": "ts-node --esm ./automation/library/build.ts",
    "library:build:generate-package-class": "generate-package-class",
    "documentation:generate": "ts-node --esm ./automation/documentation/generate.ts",
    "library:documentation:generate": "typedoc ./src/main.ts"
  },
  "keywords": [
    "cli-usage",
    "automation",
    "generation",
    "markdown-generation",
    "cli",
    "help",
    "automation",
    "generate",
    "generation",
    "markdown",
    "cli-app",
    "cli-application",
    "console-app",
    "console-application",
    "readme",
    "readme-generation",
    "markdown-generation",
    "documentation",
    "documentation-generation",
    "doc-generation",
    "npm-cli-app",
    "npm-cli-applaction",
    "npm-console-application",
    "table-of-contents",
    "toc",
    "summary",
    "table-of-contents-generation",
    "toc-generation"
  ],
  "author": "FreePhoenix888",
  "license": "Unlicense",
  "files": [
    "dist"
  ],
  "dependencies": {
    "debug": "^4.3.4",
    "fs-extra": "^11.1.1",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@atao60/fse-cli": "^0.1.7",
    "@deep-foundation/npm-automation": "^17.1.3",
    "@types/debug": "^4.1.8",
    "@types/fs-extra": "^11.0.1",
    "@types/node": "^20.4.2",
    "@types/yargs": "^17.0.24",
    "execa": "^7.1.1",
    "typedoc": "^0.24.8",
    "types-package-json": "^2.0.39",
    "typescript": "^5.1.6"
  }
}
